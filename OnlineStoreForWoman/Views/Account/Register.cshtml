    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100">
                <form method="post" class="login100-form validate-form">
                    @Html.AntiForgeryToken()

                    <span class="login100-form-title p-b-26">
                        Register
                    </span>
                    <span class="login100-form-title p-b-48">
                        <i class="zmdi zmdi-account"></i>
                    </span>

                    <div class="wrap-input100 validate-input" data-validate="Enter name">
                        @Html.TextBoxFor(m => m.Name, new { @class = "input100" })
                        <span class="focus-input100" data-placeholder="Name"></span>
                        @Html.LabelFor(m => m.Name, new { @class = "label-input100" })
                        @Html.ValidationMessageFor(m => m.Name, "", new { @class = "field-validation-error" })
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Enter username">
                        @Html.TextBoxFor(m => m.Username, new { @class = "input100" })
                        <span class="focus-input100" data-placeholder="Username"></span>
                        @Html.LabelFor(m => m.Username, new { @class = "label-input100" })
                        @Html.ValidationMessageFor(m => m.Username, "", new { @class = "field-validation-error" })
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Valid email is: a@b.c">
                        @Html.TextBoxFor(m => m.Email, new { @class = "input100", type = "email" })
                        <span class="focus-input100" data-placeholder="Email"></span>
                        @Html.LabelFor(m => m.Email, new { @class = "label-input100" })
                        @Html.ValidationMessageFor(m => m.Email, "", new { @class = "field-validation-error" })
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Enter password">
                        <span class="btn-show-pass">
                            <i class="zmdi zmdi-eye"></i>
                        </span>
                        @Html.PasswordFor(m => m.Password, new { @class = "input100" })
                        <span class="focus-input100" data-placeholder="Password"></span>
                        @Html.LabelFor(m => m.Password, new { @class = "label-input100" })
                        @Html.ValidationMessageFor(m => m.Password, "", new { @class = "field-validation-error" })
                    </div>

                    <div class="container-login100-form-btn">
                        <div class="wrap-login100-form-btn">
                            <div class="login100-form-bgbtn"></div>
                            <button type="submit" class="login100-form-btn">
                                Create
                            </button>
                        </div>
                    </div>

                    @Html.ValidationSummary("", new { @class = "field-validation-error" })
                </form>
            </div>
        </div>
    </div>

